{
  "name": "@thearchitech.xyz/cli",
  "version": "0.8.8",
  "description": "Revolutionary AI-Powered Application Generator - Transforming weeks of work into minutes",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "architech": "./bin/architech.js"
  },
  "scripts": {
    "dev": "node bin/architech.js",
    "build": "npm run clean && npm run compile && npm run copy-bin && npm run copy-scripts",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "copy-bin": "cp -r bin dist/",
    "copy-scripts": "echo 'Scripts compiled automatically by TypeScript'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests/unit",
    "start": "node bin/architech.js",
    "link": "npm link",
    "unlink": "npm unlink -g @thearchitech.xyz/cli",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting...'",
    "format": "echo 'Formatting...'",
    "validate:modules": "node dist/scripts/validate-module-loading.js"
  },
  "keywords": [
    "cli",
    "generator",
    "ai",
    "nextjs",
    "react",
    "typescript",
    "shadcn",
    "tailwind",
    "architecture",
    "development",
    "productivity"
  ],
  "author": "The Architech Team",
  "license": "AGPL-3.0-or-later",
  "preferGlobal": true,
  "dependencies": {
    "@thearchitech.xyz/genome-transformer": "file:../architech-genome-transformer",
    "@thearchitech.xyz/marketplace": "^0.9.2",
    "@thearchitech.xyz/types": "file:../types-package",
    "@types/ejs": "^3.1.5",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/parser": "^8.43.0",
    "@typescript-eslint/typescript-estree": "^8.43.0",
    "ajv": "^8.17.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "deepmerge": "^4.3.1",
    "ejs": "^3.1.10",
    "execa": "^8.0.1",
    "glob": "^10.3.10",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "ts-morph": "^27.0.0"
  },
  "optionalDependencies": {
    "@thearchitech.xyz/marketplace-shadcn": "^1.0.0",
    "@thearchitech.xyz/marketplace-tamagui": "^1.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/commander": "^2.12.5",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^20.19.11",
    "@types/pg": "^8.15.5",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "src/",
    "dist/",
    "templates/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thearchitech-xyz/cli.git"
  },
  "bugs": {
    "url": "https://github.com/thearchitech-xyz/cli/issues"
  },
  "homepage": "https://thearchitech.xyz",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
